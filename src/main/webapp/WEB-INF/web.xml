<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <display-name>Archetype Created Web Application</display-name>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
   <servlet>
     <servlet-name>RegisterServlet</servlet-name>
     <servlet-class>com.librarymanagement.servlet.RegistrationServlet</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.librarymanagement.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddBookServlet</servlet-name>
        <servlet-class>com.librarymanagement.servlet.AddBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddBookServlet</servlet-name>
        <url-pattern>/AddBook</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>ViewBooksServlet</servlet-name>
    <servlet-class>com.librarymanagement.servlet.ViewBooks</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewBooksServlet</servlet-name>
        <url-pattern>/ViewBooksServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AdminProfile</servlet-name>
        <servlet-class>com.librarymanagement.servlet.AdminProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminProfile</servlet-name>
        <url-pattern>/AdminProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteBookServlet</servlet-name>
        <servlet-class>com.librarymanagement.servlet.DeleteBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteBookServlet</servlet-name>
        <url-pattern>/DeleteBookServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StudentProfileServlet</servlet-name>
        <servlet-class>com.librarymanagement.servlet.StudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentProfileServlet</servlet-name>
        <url-pattern>/StudentServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchBookServletStudent</servlet-name>
        <servlet-class>com.librarymanagement.servlet.SearchIssuedBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchBookServletStudent</servlet-name>
        <url-pattern>/SearchIssuedBookServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>IssueBookServlet</servlet-name>
        <servlet-class>com.librarymanagement.servlet.IssueSingleBook</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IssueBookServlet</servlet-name>
        <url-pattern>/IssuedBook</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RenewBookServlet</servlet-name>
        <servlet-class>com.librarymanagement.servlet.RenewBook</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RenewBookServlet</servlet-name>
        <url-pattern>/RenewBook</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EditBook</servlet-name>
        <servlet-class>com.librarymanagement.servlet.EditBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditBook</servlet-name>
        <url-pattern>/EditBookServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchBook</servlet-name>
        <servlet-class>com.librarymanagement.servlet.SearchBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchBook</servlet-name>
        <url-pattern>/SearchBookServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReturnBookServlet</servlet-name>
        <servlet-class>com.librarymanagement.servlet.ReturnBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReturnBookServlet</servlet-name>
        <url-pattern>/ReturnBook</url-pattern>
    </servlet-mapping>

</web-app>
